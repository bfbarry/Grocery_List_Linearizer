for walk in possible_paths:
        #print('walk: ', walk)
        flat = [elt for edge in walk for elt in edge] # from https://stackoverflow.com/questions/3204245/how-do-i-convert-a-tuple-of-tuples-to-a-one-dimensional-list-using-list-comprehe
        #print('flat: ', flat)
        unique = set(flat) #unique vertices from flattened list
        counts = [flat.count(i) for i in flat]
